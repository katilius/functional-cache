{
  "name": "functional-cache",
  "version": "0.2.2",
  "description": "Set of utility functions that let you decorate functions with cache",
  "main": "./dist/index.js",
  "scripts": {
    "prepublishOnly": "npm run test && npm run clean && npm run build",
    "build": "babel ./src -d ./dist --ignore **/*.test.js",
    "test": "jest",
    "clean": "rimraf dist",
    "test:publish-coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "format": "prettier --write \"src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katilius/functional-cache.git"
  },
  "keywords": [
    "cache",
    "functional",
    "evict",
    "conditional-caching"
  ],
  "author": "Arminas Katilius",
  "license": "MIT",
  "dependencies": {
    "lru-cache": "4.1.3"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "coveralls": "3.0.2",
    "husky": "1.2.0",
    "jest": "23.6.0",
    "lint-staged": "7.2.2",
    "prettier": "1.15.2",
    "rimraf": "2.6.2"
  },
  "prettier": {
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
